    "n = int(input(\"Enter a Number\"))\n",
    "\n",
    "original = n\n",
    "sumofn = 0\n",
    "\n",
    "\n",
    "while n > 0:\n",
    "    digit = n %10\n",
    "    sumofn += digit\n",
    "    n = n //10\n",
    "\n",
    "print(\"original value\",original)\n",
    "print(\"sum of numbers\",sumofn)"
